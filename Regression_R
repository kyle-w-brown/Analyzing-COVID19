# Regression for Confirmed Cases

fit <- lm(Confirmed ~ Asthma_Total + Deaths + Population_2018, data=covid_all_confirmed) 
summary(fit) # show results


#Residuals:
#  Min      1Q  Median      3Q     Max 
#-5343.7  -257.1    25.3   225.6  4368.5 

#Coefficients:
#  Estimate Std. Error t value Pr(>|t|)    
#(Intercept)     -9.431e+00  3.128e+02  -0.030    0.976    
#Asthma_Total    -2.564e-03  3.401e-03  -0.754    0.455    
#Deaths           3.852e+01  8.232e-01  46.797   <2e-16 ***
#  Population_2018  2.318e-04  2.168e-04   1.069    0.290    
#---
#  Signif. codes:  0 ‘***’ 0.001 ‘**’ 0.01 ‘*’ 0.05 ‘.’ 0.1 ‘ ’ 1

#Residual standard error: 1518 on 47 degrees of freedom
#Multiple R-squared:  0.9877,	Adjusted R-squared:  0.987 
#F-statistic:  1262 on 3 and 47 DF,  p-value: < 2.2e-16


modelSummary <- summary(fit)  # capture model summary as an object
modelCoeffs <- modelSummary$coefficients  # model coefficients
beta.estimate <- modelCoeffs["Deaths", "Estimate"]  # get beta estimate for speed
std.error <- modelCoeffs["Deaths", "Std. Error"]  # get std.error for speed
t_value <- beta.estimate/std.error  # calc t statistic
p_value <- 2*pt(-abs(t_value), df=nrow(cars)-ncol(cars))  # calc p Value
f_statistic <- fit$fstatistic[1]  # fstatistic
f <- summary(fit)$fstatistic  # parameters for model p-value calc
model_p <- pf(f[1], f[2], f[3], lower=FALSE)

