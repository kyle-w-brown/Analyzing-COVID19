fit2 <- lm(Deaths ~  Adult_Physical_Activity	+ Adult_Smoking+	Cancer_Death_Rate+	Child_Vaccination_Coverage	+State_Standardized_Infection_Ratios	+Colorectal_Cancer_Screening_Pct +	Diagnosed_Diabetes_Pct +	Diagnosed_High_Cholesterol_Pct, data=covid_all_confirmed) 
