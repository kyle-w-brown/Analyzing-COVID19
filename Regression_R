# Regression with our dataset in repo

covid_all_confirmed = read.csv("https://raw.githubusercontent.com/WorldCapital/COVID19-Project/master/COVID19_Confirmed.csv")

# Regression COVID using Deaths

fit <- lm(Deaths ~ Asthma_Total + Confirmed + Population_2018, data=covid_all_confirmed) 
summary(fit) # show results

#Residuals:
#  Min       1Q   Median       3Q      Max 
#-101.660   -6.172   -0.091    8.106  132.074 

#Coefficients:
#  Estimate Std. Error t value Pr(>|t|)    
#(Intercept)     -1.609e+00  8.031e+00  -0.200    0.842    
#Asthma_Total     1.140e-04  8.629e-05   1.321    0.193    
#Confirmed        2.541e-02  5.430e-04  46.797   <2e-16 ***
#  Population_2018 -8.720e-06  5.490e-06  -1.588    0.119    
#---
#  Signif. codes:  0 ‘***’ 0.001 ‘**’ 0.01 ‘*’ 0.05 ‘.’ 0.1 ‘ ’ 1

#Residual standard error: 38.99 on 47 degrees of freedom
#Multiple R-squared:  0.9877,	Adjusted R-squared:  0.9869 
#F-statistic:  1256 on 3 and 47 DF,  p-value: < 2.2e-16


modelSummary <- summary(fit)  # capture model summary as an object
modelCoeffs <- modelSummary$coefficients  # model coefficients
beta.estimate <- modelCoeffs["Confirmed", "Estimate"]  # get beta estimate for speed
std.error <- modelCoeffs["Confirmed", "Std. Error"]  # get std.error for speed
t_value <- beta.estimate/std.error  # calc t statistic
p_value <- 2*pt(-abs(t_value), df=nrow(cars)-ncol(cars))  # calc p Value
f_statistic <- fit$fstatistic[1]  # fstatistic
f <- summary(fit)$fstatistic  # parameters for model p-value calc
model_p <- pf(f[1], f[2], f[3], lower=FALSE)

t_value
#> t_value
#[1] 46.79714

p_value
#> p_value
#[1] 1.039563e-41

f
#> f
#value    numdf    dendf 
#1256.462    3.000   47.000 

model_p
#> model_p
#value 
#7.374166e-45


#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

fit1 <- lm(Deaths ~  Unemployed +Poverty +	Median_Income +	Adult_Binge_Drinking_Pct + Adult_Nutrition +	Adult_Obesity, data=covid_all_confirmed) 
summary(fit1) # show results

# Call:
#  lm(formula = Deaths ~ Unemployed + Poverty + Median_Income + 
#       Adult_Binge_Drinking_Pct + Adult_Nutrition + Adult_Obesity, 
#     data = covid_all_confirmed)

#Residuals:
#  Min      1Q  Median      3Q     Max 
#-451.88 -102.84  -35.85   19.55 2041.44 

#Coefficients:
#  Estimate Std. Error t value Pr(>|t|)
#(Intercept)              -1.855e+02  1.154e+03  -0.161    0.873
#Unemployed                2.905e+01  8.282e+01   0.351    0.727
#Poverty                   2.303e+01  3.860e+01   0.597    0.554
#Median_Income             7.207e-03  9.356e-03   0.770    0.445
#Adult_Binge_Drinking_Pct -9.755e+00  1.821e+01  -0.536    0.595
#Adult_Nutrition           2.979e+01  2.277e+01   1.308    0.198
#Adult_Obesity            -3.536e+01  2.362e+01  -1.497    0.142

#Residual standard error: 341.9 on 44 degrees of freedom
#Multiple R-squared:  0.1135,	Adjusted R-squared:  -0.007339 
#F-statistic: 0.9393 on 6 and 44 DF,  p-value: 0.4769



modelSummary <- summary(fit1)  # capture model summary as an object
modelCoeffs <- modelSummary$coefficients  # model coefficients
beta.estimate <- modelCoeffs["Deaths", "Estimate"]  # get beta estimate 
std.error <- modelCoeffs["Confirmed", "Std. Error"]  # get std.error 
t_value <- beta.estimate/std.error  # calc t statistic
p_value <- 2*pt(-abs(t_value), df=nrow(Deaths)-ncol(Deaths))  # calc p Value
f_statistic <- fit1$fstatistic[1]  # fstatistic
f <- summary(fit1)$fstatistic  # parameters for model p-value calc
model_p <- pf(f[1], f[2], f[3], lower=FALSE)

t_value
#> t_value
#[1] 46.79714

p_value
#> p_value
#[1] 1.039563e-41

f
#> f
#value    numdf    dendf 
#1256.462    3.000   47.000 

model_p
#> model_p
#value 
#7.374166e-45


