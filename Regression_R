fit4 <- lm(Deaths ~  Teen_Birth_Rate + Youth_Binge_Drinking + Youth_Marijuana_Use + Youth_Nutrition + Youth_Obesity + Youth_Physical_Activity + Youth_Seat_Belt_Use + Youth_Smoking, data=covid_all_confirmed) 
summary(fit4) # show results
print(fit4)

